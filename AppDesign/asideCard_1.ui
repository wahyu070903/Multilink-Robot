   <property name="styleSheet">
    <string notr="true">QSlider:horizontal {
    /* Add space on the sides for the large handle to be fully visible at min/max positions */
    padding: 0 10px;
}

QSlider::groove:horizontal {
    /* --- TRACK (GROOVE) STYLING --- */
    
    /* Uniform light gray color for the entire track as shown in the image */
    background: #e0e0e0;
    
    /* Set the track height */
    height: 8px;
    
    /* Fully rounded ends */
    border-radius: 4px;
    
    /* Prevent borders from showing */
    border: none;
}

/* Ensure the sub-page (filled portion) and add-page (unfilled portion) match the groove */
QSlider::sub-page:horizontal,
QSlider::add-page:horizontal {
    background: #e0e0e0;
    border-radius: 4px;
}

QSlider::handle:horizontal {
    /* --- HANDLE (THUMB) STYLING --- */
    
    /* Dimensions: 20px diameter circle */
    width: 20px;
    height: 20px;
    
    /* Center the 20px handle vertically on the 8px groove: (20 - 8) / 2 = 6px negative margin */
    margin: -8px 0;
    
    /* Make it a perfect circle */
    border-radius: 10px;
    
    /* White center fill */
    background: white;
    
    /* The bright purple ring/outline */
    border: 3px solid #7e55ff;
    
    /* The crucial glow effect using box-shadow */
    box-shadow: 0 0 8px rgba(126, 85, 255, 0.6);
}

/* Optional: Subtle changes when the user hovers over the handle */
QSlider::handle:horizontal:hover {
    border-color: #6a3dff; /* Slightly darker purple on hover */
    box-shadow: 0 0 10px rgba(106, 61, 255, 0.8); /* Stronger glow on hover */
}
</string>
